{
  "name": "@alexandernanberg/react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "author": "Alexander Nanberg <alex.nanberg@hotmail.com> (https://alexandernanberg.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexandernanberg/react-boilerplate"
  },
  "scripts": {
    "dev": "react-toolkit",
    "build": "react-toolkit build",
    "lint": "eslint 'src/**/*.js' && stylelint 'src/**/*.js'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "stylelint"
    ]
  },
  "eslintConfig": {
    "extends": [
      "alexandernanberg"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-alexandernanberg"
    ]
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions"
  ],
  "dependencies": {
    "history": "^5.0.0",
    "preval.macro": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-router-dom": "^6.0.0-beta.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.18.0",
    "eslint-config-alexandernanberg": "^1.12.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react-toolkit": "^2.0.0-alpha.4",
    "stylelint": "^13.8.0",
    "stylelint-config-alexandernanberg": "^1.5.6"
  }
}
