{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "semi": ["error", "never"],
    "react/jsx-closing-bracket-location": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0
  }
}
